{
  "name": "meeting-room-reservation-app",
  "version": "1.0.0",
  "description": "test project for Adlin Science",
  "homepage": "https://github.com/codeIsHard2023/meeting-room-reservation/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/codeIsHard2023/meeting-room-reservation/tree/main"
  },
  "scripts": {
    "dev": "cross-env-shell 'concurrently -c green,yellow -t \"HH:mm:ss\" -p \"{name} {time}\" \"npm:dev-*\"'",
    "dev-front": "cross-env-shell \"cd ./client && npm run dev\"",
    "dev-back": "cross-env-shell \"cd ./server && npm run dev\"",
    "lint": "lint-staged",
    "prebuild": "npm install",
    "build": "cross-env-shell \"cd ./client && npm run build && cd ../server && npm run build\"",
    "start": "cross-env-shell \"cd ./server && npm run start\"",
    "clean": "cross-env-shell \"rm -rf ./node_modules ./package-lock.json ./server/node_modules ./server/package-lock.json ./client/node_modules ./client/package-lock.json\""
  },
  "keywords": [],
  "author": "Vladyslav TEZYK",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "lint-staged": "^13.2.3"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^8.4.0",
    "axios": "^1.6.7",
    "eslint": "^9.0.0",
    "pinia": "^2.1.7",
    "prettier": "^3.2.5",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  }
}
